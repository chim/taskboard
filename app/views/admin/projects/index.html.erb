<script>
	projects_search = new Search('project-container');
</script>
<% if @projects.empty? -%>
<%= admin_open_div(750)%>
<div id="no-projects">
	<h1>No projects found.</h1>
	<p>You're not participating in any projects right now, please contact an administrator of your organization.</p>
</div>
<%= admin_close_div(750)%>
<% end -%>
<div id="projects-list">
<%= render :partial => "project", :collection => @projects, :locals => { :member => @member } %>
</div>
