<% 
element_class = element.class.to_s.downcase
parent_class = parent.class.to_s.downcase
%>

<div id='<%= parent_class -%>-<%= parent.id -%>-<%= element_class -%>-<%= element.id -%>' class='admin-div-element admin-div-element-core'>
<%
# Write the name of the element
%>
	<div id="name-<%= parent_class%>-<%= parent.id %>-<%= element_class -%>-<%= element.id -%>-container">
		<p id="name-<%= parent_class%>-<%= parent.id %>-<%= element_class -%>-<%= element.id -%>" title="<%= element.name -%>">
			<% if defined?(contact) && contact %>
				<a href="mailto:<%= element.email %>">
				<%= image_tag "admin/admin-div-actions-email-small.png", :alt => "email", :title => "Send E-Mail to Administrator" -%>
			<% end -%>
				<%= element.name -%>
			<% if defined?(contact) && contact %>
				</a>
			<% end %>
				
		</p>
	</div>
	<script>
		resizeFontSizeToFit($('name-<%= parent_class%>-<%= parent.id %>-<%= element_class -%>-<%= element.id -%>'),$('name-<%= parent_class%>-<%= parent.id %>-<%= element_class -%>-<%= element.id -%>-container'));
		$('name-<%= parent_class%>-<%= parent.id %>-<%= element_class -%>-<%= element.id -%>').setStyle({ width: '100%' });
	</script>
</div>
