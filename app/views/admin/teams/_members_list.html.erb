<div id="member-list-faces" style="width: 700px; height: 125px;">	
	<% 
		member_count = 0 
		member_id_list = Array.new
	-%>
	<% members.each do |member| %>
		<div id="drag-member-<%= member.id %>" class="member">
			<div id="member-list-<%= member_count %>" class=" member-container" style="background: url('/images/admin/admin-teams-member-background.png') no-repeat; width: 109px; height: 125px; padding: 10px; float: left;" title="<%= member.name -%>">
				<%= render :partial => "admin/members/show_with_picture", 
					:locals => { 
						:id_text => "organization-member", 
						:class_text => "organization_member", 
						:member_name_class => "organization-member",
						:member => member,
						:edit => false } -%>
			</div>
		</div>	
		<%= draggable_element "drag-member-#{member.id}", :revert => "'failure'" -%>
		<% 
			member_id_list << "member-list-#{member_count}"
			member_count += 1 
		-%>
	<% end %>
	<div style="margin-top: 10px; width: 40px; z-index: 100; position: absolute; top: 140px; left: 640px;">
		<%= image_tag "more.png", :id => "members_slider_next",:alt => "more", :class => "more", :style => "border: none; position: absolute; top: 0px;", :onClick => "members_slider.next();" -%>
		<%= image_tag "less.png", :id => "members_slider_prev", :alt => "less", :class => "less", :style => "border: none; position: absolute; top: 45px;", :onClick => "members_slider.prev();" %>
	</div>
	<script>
		members_slider = new Slider(<%= member_id_list.to_json %>,5,"members_slider");
	</script>
	</div>
